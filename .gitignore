import math
def calculate(a, b, operation):
   result = None
 
   if operation == '+':
       result = sum(a, b)
 
   elif operation == '-':
       result = subtract(a, b)
  
   elif operation == '/':
       result = divide(a, b)
 
   elif operation == '*':
       result = multiply(a, b)
 
   elif operation == '**':
       result = pow(a, b)

   elif operation == '//':
       result = pow(a,(1/b))
   
   else:
       print('Неизвестная операция')
 
   return result
